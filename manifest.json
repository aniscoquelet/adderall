{
    "name": "Adderall",
    "description":"Virtual pill for all your focusing needs.",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_popup" : "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "css": ["youtube/css/youtube.css"],
        "js": ["youtube/js/youtubeHomepage.js"]
      }
    ],
  "icons": { "16": "icons/16.png",
    "128": "icons/128.png" }
}